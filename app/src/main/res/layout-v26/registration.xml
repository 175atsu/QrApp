<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">
    <Button
            android:text="保存"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/save"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="58dp" android:onClick="save"/>
    <TextView
            android:text="name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8" app:layout_constraintEnd_toStartOf="@+id/name_text"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="112dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.344"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            android:layout_marginTop="68dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10" app:layout_constraintEnd_toStartOf="@+id/github_text"
            android:layout_marginEnd="44dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="42dp"/>
    <TextView
            android:text="Twitter ID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="40dp" android:layout_marginTop="72dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/nameData"
            android:ems="10"
            android:id="@+id/name_text" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="53dp" android:layout_marginTop="99dp" app:layout_constraintTop_toTopOf="parent"
            android:autofillHints=""/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/TwitterData"
            android:ems="10"
            android:id="@+id/twitter_text" android:layout_marginTop="42dp"
            app:layout_constraintTop_toBottomOf="@+id/name_text" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="53dp" android:layout_marginBottom="36dp"
            app:layout_constraintBottom_toTopOf="@+id/github_text" app:layout_constraintStart_toEndOf="@+id/textView10"
            android:layout_marginStart="25dp" app:layout_constraintHorizontal_bias="1.0"
            android:importantForAutofill="no"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/GithubData"
            android:ems="10"
            android:id="@+id/github_text" android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/twitter_text" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="53dp" android:autofillHints=""/>
</androidx.constraintlayout.widget.ConstraintLayout>