<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:background="@drawable/style_back"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent" android:id="@+id/Constraint">
    <Button
            android:text="保存"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="58dp"
            android:id="@+id/btnSave"/>
    <ImageView
            android:layout_width="340dp"
            android:layout_height="550dp"
            android:background="@drawable/style_edit"
            android:transitionName="image3"
            android:id="@+id/imageView2" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp" android:layout_marginTop="55dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"/>
    <TextView
            android:text="GithubID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp"/>
    <TextView
            android:text="Twitter ID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="70dp" app:layout_constraintTop_toBottomOf="@+id/textView8"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/name_text" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="53dp" android:layout_marginTop="99dp" app:layout_constraintTop_toTopOf="parent"
            android:autofillHints=""/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/twitter_text" android:layout_marginTop="42dp"
            app:layout_constraintTop_toBottomOf="@+id/name_text" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="53dp"
            android:importantForAutofill="no"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/github_text"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="53dp" android:autofillHints="" android:layout_marginTop="42dp"
            app:layout_constraintTop_toBottomOf="@+id/twitter_text"/>
    <TextView
            android:text="個人データ登録"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="20dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="38dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>